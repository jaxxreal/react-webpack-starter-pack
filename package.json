{
  "name": "react-webpack-starter-pack",
  "version": "1.0.0",
  "description": "Starter pack for friends",
  "main": "webpack.config.base.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --progress --watch",
    "build": "webpack --config webpack.config.prod.js --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaxxreal/react-webpack-starter-pack.git"
  },
  "author": "Distillery",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaxxreal/react-webpack-starter-pack/issues"
  },
  "homepage": "",
  "dependencies": {
    "axios": "0.15.3",
    "babel-polyfill": "6.20.0",
    "classnames": "2.2.5",
    "immutable": "3.8.1",
    "js-cookie": "2.1.3",
    "lodash": "4.17.4",
    "normalize.css": "5.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.1",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-immutable": "3.0.10",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
